#include <iostream>

std::string createFullAddress(std::string street, std::string city, std::string country) {
    street.append(" street, ").append(city).append(" city, ").append(country);
    return street;
}

int main() {
    std::cout << createFullAddress("123 Streeet", "New York", "USA") << std::endl;
    std::cout << createFullAddress("456 Main St", "Los Angeles", "USA") << std::endl;
    return 0;
}